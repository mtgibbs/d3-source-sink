{
  "name": "d3-source-sink",
  "version": "3.0.1",
  "description": "D3 plugin that generates a source to sink graph for following flow.  Similar to a Sankey Visualization.",
  "main": "build/d3-source-sink.js",
  "scripts": {
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "start": "tsc && concurrently \"npm run tsc:w\"",
    "clean": "rimraf dist",
    "lint": "tslint -c ./tslint.json ./**/*.ts --exclude \"./node_modules/**/*\"",
    "build": "tsc && uglifyjs --compress --mangle --output ./build/d3-source-sink.min.js -- ./build/d3-source-sink.js"
  },
  "keywords": [
    "d3",
    "d3-module"
  ],
  "author": "Matt Gibbs <matt@mtgibbs.xyz> (http://mtgibbs.xyz)",
  "devDependencies": {
    "concurrently": "2.2.0",
    "rimraf": "2.5.4",
    "tslint": "3.15.1",
    "typescript": "2.0.3",
    "uglify-js": "2.7.3"
  },
  "dependencies": {
    "@types/d3": "3.5.36"
  }
}
